  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --coverage --runInBand --config ./jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./type-orm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./type-orm.config.ts migration:generate ./src/migrations/%npm_config_name%"
  },
